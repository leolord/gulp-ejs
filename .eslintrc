{
  "globals" : {
    "define"  : false,
    "require" : false,
    "$"       : false,
    "_"       : false
  },
  "ecmaFeatures": {
    "arrowFunctions"                   : true,
    "binaryLiterals"                   : true,
    "blockBindings"                    : true,
    "classes"                          : true,
    "defaultParams"                    : true,
    "destructuring"                    : true,
    "forOf"                            : true,
    "generators"                       : true,
    "globalReturn"                     : false,
    "jsx"                              : true,
    "modules"                          : true,
    "objectLiteralComputedProperties"  : true,
    "objectLiteralDuplicateProperties" : false,
    "objectLiteralShorthandMethods"    : true,
    "objectLiteralShorthandProperties" : false,
    "octalLiterals"                    : true,
    "regexUFlag"                       : true,
    "regexYFlag"                       : false,
    "restParams"                       : true,
    "spread"                           : true,
    "superInFunctions"                 : true,
    "templateStrings"                  : true,
    "unicodeCodePointEscapes"          : true,
  },
  "env" : {
    "browser" : true,
    "node"    : true,
    "jquery"  : true,
    "es6"     : true,
    "amd"     : true
  },
  "globals": {
    "$": true,
    "_": true
  },
  "rules": {
    "accessor-pairs"             : 0,
    "block-scoped-var"           : 0,
    "camelcase"                  : [2, {"properties": "always"}],
    "comma-dangle"               : [2, "never"],
    "comma-spacing"              : 1,
    "complexity"                 : [1, 4],
    "curly"                      : [1, "multi-line"],
    "dot-notation"               : [2, {"allowKeywords": false}],
    "eqeqeq"                     : [2, "smart"],
    "global-strict"              : [2, "always"],
    "guard-for-in"               : 2,
    "indent"                     : [2, 2],
    "key-spacing"                : 0,
    "max-depth"                  : [1, 3],
    "max-len"                    : [1, 120, 2],
    "max-nested-callbacks"       : [2, 3],
    "max-params"                 : [1, 3],
    "max-statements"             : [1, 20],
    "new-cap"                    : 1,
    "new-parens"                 : 1,
    "no-alert"                   : 0,
    "no-array-constructor"       : 2,
    "no-caller"                  : 0,
    "no-catch-shadow"            : 2,
    "no-cond-assign"             : [2, "except-parens"],
    "no-constant-condition"      : 2,
    "no-debugger"                : 0,
    "no-delete-var"              : 2,
    "no-div-regex"               : 1,
    "no-dupe-args"               : 2,
    "no-dupe-keys"               : 2,
    "no-duplicate-case"          : 2,
    "no-else-return"             : 1,
    "no-empty"                   : 0,
    "no-empty-character-class"   : 2,
    "no-empty-label"             : 2,
    "no-eq-null"                 : 0,
    "no-eval"                    : 2,
    "no-ex-assign"               : 1,
    "no-extend-native"           : 2,
    "no-extra-bind"              : 1,
    "no-extra-boolean-cast"      : 2,
    "no-extra-semi"              : 1,
    "no-fallthrough"             : 2,
    "no-floating-decimal"        : 2,
    "no-func-assign"             : 2,
    "no-implied-eval"            : 2,
    "no-inner-declarations"      : 2,
    "no-invalid-regexp"          : 2,
    "no-irregular-whitespace"    : 1,
    "no-iterator"                : 2,
    "no-label-var"               : 1,
    "no-labels"                  : 1,
    "no-lone-blocks"             : 2,
    "no-lonely-if"               : 1,
    "no-loop-func"               : 2,
    "no-multi-spaces"            : 0,
    "no-multi-str"               : 2,
    "no-native-reassign"         : 2,
    "no-negated-in-lhs"          : 2,
    "no-nested-ternary"          : 2,
    "no-new"                     : 0,
    "no-new-func"                : 2,
    "no-new-object"              : 2,
    "no-new-wrappers"            : 2,
    "no-obj-calls"               : 2,
    "no-octal"                   : 0,
    "no-octal-escape"            : 2,
    "no-param-reassign"          : 1,
    "no-proto"                   : 2,
    "no-redeclare"               : 2,
    "no-regex-spaces"            : 1,
    "no-reserved-keys"           : 2,
    "no-return-assign"           : 2,
    "no-script-url"              : 0,
    "no-self-compare"            : 2,
    "no-sequences"               : 2,
    "no-shadow"                  : 2,
    "no-shadow-restricted-names" : 2,
    "no-sparse-arrays"           : 2,
    "no-throw-literal"           : 2,
    "no-trailing-spaces"         : 0,
    "no-undef"                   : 2,
    "no-undef-init"              : 1,
    "no-underscore-dangle"       : 0,
    "no-unneeded-ternary"        : 2,
    "no-unreachable"             : 2,
    "no-unused-expressions"      : 2,
    "no-unused-vars"             : 1,
    "no-use-before-define"       : 2,
    "no-void"                    : 2,
    "no-with"                    : 2,
    "operator-linebreak"         : [2, "before"],
    "quotes"                     : [1, "single", "avoid-escape"],
    "radix"                      : 2,
    "semi"                       : 2,
    "space-infix-ops"            : [1, {"int32Hint": false}],
    "use-isnan"                  : 2,
    "valid-typeof"               : 2,
    "vars-on-top"                : 0,
    "wrap-iife"                  : [2, "inside"],
  }
}
